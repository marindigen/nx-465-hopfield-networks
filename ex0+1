   "source": [
    "#TO DELETE\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def simulate_hopfield(N, M, iterations=10):\n",
    "    retrieved_patterns = []\n",
    "    for _ in range(iterations):\n",
    "        patterns = generate_patterns(M, N)\n",
    "        initial_state = patterns[0]  # Simplification: using the first pattern as initial state\n",
    "        current_state = initial_state.copy()\n",
    "        for _ in range(50):  # Number of steps to reach stability\n",
    "            current_state = next_state_optimised(current_state, patterns)\n",
    "        # Calculate retrieval success rate\n",
    "        correct_retrievals = sum(distance_hamming(current_state, pattern) <= 0.05 for pattern in patterns)\n",
    "        retrieved_patterns.append(correct_retrievals / N)\n",
    "    return np.mean(retrieved_patterns), np.std(retrieved_patterns)\n",
    "\n",
    "# Parameters and simulation\n",
    "Ns = range(50, 801, 150)  # Network sizes\n",
    "Ls = [0.10, 0.12, 0.14, 0.16, 0.18]  # Loadings to test\n",
    "results = {N: {} for N in Ns}\n",
    "\n",
    "for N in Ns:\n",
    "    for L in Ls:\n",
    "        M = int(L * N)\n",
    "        mean_retrievals, std_retrievals = simulate_hopfield(N, M)\n",
    "        results[N][L] = (mean_retrievals, std_retrievals)\n",
    "\n",
    "# Plotting the results\n",
    "plt.figure(figsize=(10, 6))\n",
    "for N in Ns:\n",
    "    loadings = []\n",
    "    means = []\n",
    "    stds = []\n",
    "    for L, (mean, std) in results[N].items():\n",
    "        loadings.append(L)\n",
    "        means.append(mean)\n",
    "        stds.append(std)\n",
    "    plt.errorbar(loadings, means, yerr=stds, label=f'N={N}', marker='o')\n",
    "\n",
    "plt.title('Retrieved Patterns per Neuron vs. Loading L')\n",
    "plt.xlabel('Loading L (M/N)')\n",
    "plt.ylabel('Retrieved Patterns per Neuron')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  },
